{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.ts",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install && npx prisma generate",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/src/index.ts" }]
}
